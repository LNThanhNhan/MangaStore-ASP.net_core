<!DOCTYPE html>
<html lang="vi">
<head>
    <title>@ViewData["Title"] MangaStore</title>
    <link rel="icon" type="image/png" href="~/favicon/mangastore.ico" />
    <partial name="AdminHeader"/>
    <script src="http://code.jquery.com/jquery.min.js" type="text/javascript"></script>
    @await RenderSectionAsync("css", required: false)
</head>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">
    <partial name="AdminSidebar"/>
    @RenderBody()
    <partial name="AdminFooter"/>
</div>
    @await RenderSectionAsync("js", required: false)
</body>
</html>